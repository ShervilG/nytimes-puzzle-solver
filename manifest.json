{
    "manifest_version": 3,
    "name": "NewYork Times Puzzle Solver",
    "description": "Get solutions to present day's NewYork Times puzzles",
    "version": "1.0",
    "action": {
        "default_popup": "popup.html",
        "default_icon": "icon.png"
    },
    "icons": {
        "128": "icon.png"
    },
    "host_permissions": [
        "https://*/*"
    ]
}
